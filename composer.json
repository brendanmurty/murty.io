{
  "name": "brendanmurty/murty.io",
  "author": "Brendan Murty <b@murty.io> (https://b.murty.io/)",
  "homepage": "https://murty.io/",
  "description": "The Murty website built with Laravel.",
  "license": "SEE LICENSE IN license.md",
  "type": "project",
  "require": {
    "php": "^7.2.5 || ^7.3.0 || ^7.4.0",
    "fideloper/proxy": "^4.0",
    "graham-campbell/markdown": "^11.1",
    "htmlmin/htmlmin": "^7.0",
    "illuminate/view": "^7.0",
    "laravel/framework": "^7.3",
    "laravel/helpers": "^1.1",
    "laravel/tinker": "^2.0",
    "symfony/http-kernel": "5.1.5"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "nunomaduro/collision": "^4.1"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "todo": [
      "grep -ri --exclude-dir={node_modules,vendor,.git} --exclude=composer.json \"TODO\" ."
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
